    "better-auth": "^0.3.2",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/tsconfig": "^3.1.0",
    "@docusaurus/types": "^3.9.2",
    "@types/node": "^18.19.0",
